{
  "include": ["app/**/*"],

  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "noEmit": true,
    "outDir": "tmp",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "types": ["vite/client"]
  }
}
